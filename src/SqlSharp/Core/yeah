if (typeof(ITeam).IsAssignableFrom(join.Value.PropertyType) && typeof(ITeam).IsAssignableFrom(join.Value.MapsFrom.PropertyType))
					{
						sql += $" AND {config.QuotePrefix}{join.Value.ParentNamespaceAlias}{config.QuoteSuffix}.{config.QuotePrefix}TeamId{config.QuoteSuffix} = {config.QuotePrefix}{join.Key}{config.QuoteSuffix}.{config.QuotePrefix}TeamId{config.QuoteSuffix}";
					}
					else if (typeof(ITeam).IsAssignableFrom(join.Value.PropertyType) && typeof(ITeam).IsAssignableFrom(type))
					{
						sql += $" AND {config.QuotePrefix}{rootAlias}{config.QuoteSuffix}.{config.QuotePrefix}TeamId{config.QuoteSuffix} = {config.QuotePrefix}{join.Key}{config.QuoteSuffix}.{config.QuotePrefix}TeamId{config.QuoteSuffix}";
					}
					else if (typeof(ITeam).IsAssignableFrom(join.Value.MapsFrom.PropertyType) && typeof(ITeam).IsAssignableFrom(type))
					{
						sql += $" AND {config.QuotePrefix}{rootAlias}{config.QuoteSuffix}.{config.QuotePrefix}TeamId{config.QuoteSuffix} = {config.QuotePrefix}{join.Value.ParentNamespaceAlias}{config.QuoteSuffix}.{config.QuotePrefix}TeamId{config.QuoteSuffix}";
					}